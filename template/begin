	<script type="text/javascript">
		$(document).ready(function (){
				$(".note").hide();
		});
		$(function (){
			$("h6 a").click(function (){
                    $(".note").hide();
					$(this.hash).slideDown();
					return false;
			});
		});
		$(function (){
            $(".note > .uplink > a ").click(function (){
					$(this.hash).slideUp();
					return false;
			});
		});
	</script>
